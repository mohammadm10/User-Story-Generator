{
  "name": "express-server-example",
  "version": "1.0.0",
  "description": "A simple ExpressJS server in TypeScript with nodemon",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon --watch src -e ts --exec \"ts-node\" src/index.ts",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.13.1",
    "@types/node": "^20.1.7",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.0",
    "isomorphic-fetch": "^3.0.0",
    "jwks-rsa": "^3.0.1",
    "node-fetch": "^3.3.1",
    "path": "^0.12.7",
    "sweetalert2": "^11.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/isomorphic-fetch": "^0.0.36",
    "nodemon": "^2.0.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.0"
  }
}
